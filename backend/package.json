{
    "name": "brand-visibility-backend",
    "version": "1.0.0",
    "description": "Backend API for Brand Visibility Analyzer",
    "main": "dist/server.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint src --ext .ts",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "keywords": [
        "brand",
        "analytics",
        "api"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.13.2",
        "cheerio": "^1.1.2",
        "cors": "^2.8.5",
        "dns-lookup": "^0.1.0",
        "express": "^4.18.2",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.6",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "prettier": "^3.1.1",
        "ts-jest": "^29.1.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3"
    }
}
